<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>project1</display-name>
	<welcome-file-list>
		<welcome-file>/bodycontent/authentication/login.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>homeservlet</servlet-name>
		<servlet-class>project1.servlet.HomeServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>logoutservlet</servlet-name>
		<servlet-class>project1.servlet.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>loginservlet</servlet-name>
		<servlet-class>project1.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>registerservlet</servlet-name>
		<servlet-class>project1.servlet.RegisterServlet</servlet-class>
	</servlet>

</web-app>